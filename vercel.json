{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 10,
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "main.py",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            }
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "MAX_WORKERS": "1",
        "PYTHONUNBUFFERED": "1"
    }
} 